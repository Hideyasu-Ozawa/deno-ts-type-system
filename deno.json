{
  "imports": {
    "@typescript-eslint/typescript-estree": "npm:@typescript-eslint/typescript-estree",
    "@/": "./",
    "@src/": "./src/",
    "@lib/": "./lib/"
  },
  "allowJs": true,
  "compilerOptions": {
    "strict": true
  },
  "tasks": {
    "start": "deno run -A index.ts",
    "dev": "deno run -A --watch index.ts",
    "test": "deno test -A ./test/"
  }
}
